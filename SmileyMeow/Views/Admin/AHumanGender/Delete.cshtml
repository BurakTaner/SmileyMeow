@model HumanGender

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <form class="form-horizontal" asp-controller="AHumanGender" asp-action="PostDelete" method="post" asp-route-id="@Model.HumanGenderId">
        <div class="card-body">
            <h4 class="card-title">Delete Gender</h4>
            <div class="form-group row">
                <p class="sure-txt">Are you want to delete this gender ?</p>
                <label for="genderid" class="col-sm-3 text-end control-label col-form-label">Id</label>
                <div class="col-sm-9">
                    <p class="form-control" id="genderid" name="HumanGenderId">@Model.HumanGenderId</p>
                </div>
                <label for="fname" class="col-sm-3 text-end control-label col-form-label">GName</label>
                <div class="col-sm-9">
                    <p class="form-control" id="gname" name="GName">@Model.GName</p>
                </div>
            </div>
        </div>
        <div class="border-top">
            <div class="card-body sbmt-area">
                <button type="submit" class="btn btn-primary">Yes</button>
                <a type="submit" asp-controller="AHumanGender" asp-action="" class="btn btn-primary">Cancel</a>
            </div>
        </div>
    </form>
</div>
